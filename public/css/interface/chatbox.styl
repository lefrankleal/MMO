.ui-chatbox
  position absolute
  right 5px
  top 5px
  opacity 0.7
  background #040438
  border-radius 5px
  z-index 2
.ui-chatlog
  position absolute
  right 5px
  top 5px
  color white
  border-radius 5px
  z-index 3
  font 12px Arial
  .wrap
    position relative
    padding 5px 0 0 5px
    .tabs
      height 53px
      .tab
        width 45px
        height 45px
        border-radius 5px
        margin-right 3px
        background blue
        float left
      .tab:last-child
        margin 0
    .log
      color white